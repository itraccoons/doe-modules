#!/bin/sh

set -ex

PROJECT=~/projects/nomulusx

if [ x"$NOMULUS_DIR" = x ]; then
    echo "Run \"eval \$(doe nomulus env)\" first"
    exit
fi

if [ -d "$PROJECT" ]; then
    rm -rf "$PROJECT"
fi

mkdir -p "$PROJECT"
unzip "$NOMULUS_DIR"/.artifacts/google/registry/registry_alpha.ear -d "$PROJECT"

set +x