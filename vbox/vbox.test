#!/bin/sh

help="doe vbox test ..."
mod_datadir=$HOME/doe/modules/vbox/.data
mod_tempdir=$HOME/doe/.tmp/vbox

vbox_test_opts="h"

vbox_test()
{
    while getopts $vbox_test_opts o; do
	case $o in
	    '?'|'h' ) echo "$help" && return 1 ;;
	    $o      ) eval arg_opt_$o=${OPTARG} ;;
	esac
    done

    echo "Your function with your agruments"
}

vbox_test "$@" || exit 1
